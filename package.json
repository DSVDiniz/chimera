{
  "name": "my-extension",
  "displayName": "my-extension",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "My Extension",
      "properties": {
        "my-extension.scrollFastLineCount": {
          "type": "number",
          "default": 3,
          "minimum": 1,
          "maximum": 100,
          "description": "Number of lines to scroll when using Scroll Up/Down Fast commands"
        },
        "my-extension.cycleCasingOrder": {
          "type": "array",
          "default": [
            "camel",
            "pascal",
            "upper",
            "upperSnake",
            "snakePascal",
            "snakeCamel",
            "kebab",
            "upperKebab",
            "lower"
          ],
          "items": {
            "type": "string",
            "enum": [
              "camel",
              "pascal",
              "upper",
              "upperSnake",
              "snake",
              "snakePascal",
              "snakeCamel",
              "kebab",
              "upperKebab",
              "lower"
            ]
          },
          "description": "Order in which to cycle through case types. Available: camel, pascal, upper, upperSnake, snake, snakePascal, snakeCamel, kebab, upperKebab, lower"
        }
      }
    },
    "commands": [
      {
        "command": "my-extension.nextError",
        "title": "Jump to next error"
      },
      {
        "command": "my-extension.alignCursors",
        "title": "Align Cursors"
      },
      {
        "command": "my-extension.addNumbersToCursors",
        "title": "Adds numbers (counting up) to every cursor"
      },
      {
        "command": "my-extension.cycleCasing",
        "title": "Cycles casing (pascal, snake...)"
      },
      {
        "command": "my-extension.uniqueLines",
        "title": "Remove Duplicate Lines (Unique)"
      },
      {
        "command": "my-extension.reverseLines",
        "title": "Reverse Lines"
      },
      {
        "command": "my-extension.shuffleLines",
        "title": "Shuffle Lines"
      },
      {
        "command": "my-extension.sortLines",
        "title": "Sort Lines (Case Insensitive)"
      },
      {
        "command": "my-extension.sortLinesCaseSensitive",
        "title": "Sort Lines (Case Sensitive)"
      },
      {
        "command": "my-extension.scrollUpFast",
        "title": "Scroll Up Fast"
      },
      {
        "command": "my-extension.scrollDownFast",
        "title": "Scroll Down Fast"
      }
    ]
  },
  "keybindings": [
    {
      "command": "my-extension.nextError",
      "key": "ctrl+e",
      "when": "editorTextFocus"
    },
    {
      "command": "my-extension.alignCursors",
      "key": "alt+a",
      "when": "editorTextFocus"
    },
    {
      "command": "my-extension.addNumbersToCursors",
      "key": "alt+n",
      "when": "editorTextFocus"
    },
    {
      "command": "my-extension.cycleCasing",
      "key": "alt+u",
      "when": "editorTextFocus"
    },
    {
      "command": "my-extension.scrollUpFast",
      "key": "ctrl+ArrowUp",
      "when": "editorTextFocus"
    },
    {
      "command": "my-extension.scrollDownFast",
      "key": "ctrl+ArrowDown",
      "when": "editorTextFocus"
    }
  ],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "esbuild": "^0.25.8",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}